{
  "name": "canivete",
  "version": "0.0.1",
  "description": "Canivete Javascript toolkit",
  "scripts": {
    "test": "rollup -c .rollup.config.js && karma start .karma.config.js",
    "test:build": "rollup -c .rollup.config.js",
    "test:run": "karma start .karma.config.js",
    "docs": "npm run docs:dir && npm run docs:src && npm run docs:build",
    "docs:dir": "mkdir -p ./docs/src",
    "docs:src": "jsdoc ./dist -t node_modules/jsdoc-json -d ./docs/src/data.json",
    "docs:build": "node ./docs/src/makeDocs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leofavre/canivete.git"
  },
  "author": "Leonardo Favre <contato@leofavre.com> (http://www.leofavre.com/)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/leofavre/canivete/issues"
  },
  "homepage": "https://github.com/leofavre/canivete#readme",
  "devDependencies": {
    "jasmine": "~2.3.2",
    "jasmine-core": "~2.3.4",
    "jsdoc-json": "^2.0.0",
    "jsdocs": "^0.1.0",
    "karma": "~0.13.15",
    "karma-firefox-launcher": "~0.1.6",
    "karma-jasmine": "~0.3.6",
    "rollup": "^0.41.6",
    "rollup-plugin-multi-entry": "^2.0.1"
  }
}
